{
  "name": "cli-agent-orchestrator-vscode",
  "displayName": "CLI Agent Orchestrator",
  "description": "Multi-agent orchestration interface with TMUX session management",
  "version": "0.1.0",
  "publisher": "awslabs",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cliAgentOrchestrator.openDashboard"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cliAgentOrchestrator.openDashboard",
        "title": "Open Agent Orchestrator Dashboard",
        "category": "CAO"
      },
      {
        "command": "cliAgentOrchestrator.launchAgent",
        "title": "Launch New Agent",
        "category": "CAO"
      },
      {
        "command": "cliAgentOrchestrator.shutdownAll",
        "title": "Shutdown All Sessions",
        "category": "CAO"
      }
    ],
    "configuration": {
      "title": "CLI Agent Orchestrator",
      "properties": {
        "cliAgentOrchestrator.serverUrl": {
          "type": "string",
          "default": "http://localhost:9889",
          "description": "CAO Server URL"
        },
        "cliAgentOrchestrator.autoRefresh": {
          "type": "boolean",
          "default": true,
          "description": "Auto-refresh session status"
        },
        "cliAgentOrchestrator.refreshInterval": {
          "type": "number",
          "default": 2000,
          "description": "Refresh interval in milliseconds"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run compile:webview",
    "compile:webview": "cd webview && npm run build",
    "watch": "tsc -watch -p ./",
    "watch:webview": "cd webview && npm run dev",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-electron": "^2.3.8"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "ws": "^8.16.0"
  }
}
